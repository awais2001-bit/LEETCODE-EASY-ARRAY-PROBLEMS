#Find Minimum Operations to Make All Elements Divisible by Threeclass Solution:    def minimumOperations(self, nums):        count = 0        for i in range(len(nums)):            if nums[i] % 3==0:                pass            elif nums[i] < 3 or nums[i] > 3:                count += 1            return(count)s = Solution()nums = [1,2,3,4]print(s.minimumOperations(nums))        